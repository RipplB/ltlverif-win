main process cfa {
    var x : int
    var err : bool

    loc L0
    init loc IL1
    loc L1
    loc L2
    loc L3
    final loc END
    loc ERR
    error loc ERR1

    IL1 -> L0 { err := false }
    L0 -> L1 { x := 0 }
    L1 -> L2 { assume x < 5 }
    L1 -> L3 { assume not (x < 5) }
    L2 -> L1 { x := x + 1 }
    L3 -> END { assume x < 5 }
    L3 -> ERR { assume not (x < 5) }
    ERR -> ERR1 { err := true }
}